{
	"compilerOptions": {
		//"baseUrl": "./",
		//"outDir": "./dist/",
		"sourceMap": true,
		"noImplicitAny": true,
		"strictNullChecks": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"module": "esnext",
		"moduleResolution": "node",
		"pretty": true,
		"target": "es5",
		"jsx": "react",
		"typeRoots": ["types"],
		"lib": ["es7", "dom"]
	},
	"include": ["./src/**/*"],
	"exclude": ["node_modules"]
}
